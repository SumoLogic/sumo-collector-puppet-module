  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.6.0"
    }
  ],
  "pdk-version": "1.6.0",
  "template-url": "file:///opt/puppetlabs/pdk/share/cache/pdk-templates.git",
  "template-ref": "1.6.0-0-gf5564c0"

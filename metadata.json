{
  "name": "sumo-logic",
  "version": "0.2.0",
  "author": "SumoLogic",
  "summary": "Provides SumoLogic collector installation and configuration",
  "license": "Apache-2.0",
  "source": "https://github.com/SumoLogic/sumo-collector-puppet-module",
  "project_page": "https://github.com/SumoLogic/sumo-collector-puppet-module",
